<h2>
    <span class="name">{time}</span>
    <span class="date">{date}</span>
</h2>
<div class="table-holder">
    <table>
        <thead>
        <tr>
            <th class="table-cell_group">Data 1</th>
            <th class="table-cell_group">Data 2</th>
            <th class="table-cell_service">Service</th>
            <th class="table-cell_dev">Weight</th>
            <th class="table-cell_chart">Chart</th>
            <th class="table-cell_new">New trend</th>
        </tr>
        </thead>
        <tbody>
        {#data}
        <tr>
            <td class="table-cell_group">{Data1}</td>
            <td class="table-cell_group">{Data2}</td>
            <td class="table-cell_service">{Service}</td>
            <td class="table-cell_dev">{weight}</td>
            <td class="table-cell_chart"><a href="{chartLink}" target="_blank" title="Go to Chart">Chart</a></td>
            <td class="table-cell_new">{isNew}</td>
        </tr>
        {/data}
        </tbody>
    </table>
</div>